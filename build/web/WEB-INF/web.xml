<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SignupServlet</servlet-name>
        <servlet-class>com.hcl.onlineexam.controller.SignupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.hcl.onlineexam.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllStudentServlet</servlet-name>
        <servlet-class>com.hcl.onlineexam.controller.GetAllStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllCategoryServlet</servlet-name>
        <servlet-class>com.hcl.onlineexam.controller.GetAllCategoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllQuestionsServlet</servlet-name>
        <servlet-class>com.hcl.onlineexam.controller.GetAllQuestionsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignupServlet</servlet-name>
        <url-pattern>/SignupServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllStudentServlet</servlet-name>
        <url-pattern>/GetAllStudentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllCategoryServlet</servlet-name>
        <url-pattern>/GetAllCategoryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllQuestionsServlet</servlet-name>
        <url-pattern>/GetAllQuestionsServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
